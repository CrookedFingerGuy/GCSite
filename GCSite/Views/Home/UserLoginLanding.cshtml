@model ApplicationUser;


<h1>
    @Model.Email
    <br />
    @Model.gcUser.LastLogin
    <br />
    @Model.gcUser.FirstLogin
    <br />
    You have @Model.gcUser.OwnedGames.Count in your collection.
</h1>
<form asp-controller="OwnedGames" asp-action="FindGameToAdd" method="post">
    <p>
        <input type="submit" value="Add Game" />
    </p>
</form>

<p>
    <div class="btn btn-dark">
        @Html.ActionLink("Look at Collection", "Index", "OwnedGames")
    </div>
</p>

